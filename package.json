{
	"name": "bloggy",
	"version": "0.1.0",
	"description": "A Blog made with Vue and Vue Router.",
	"private": true,
	"scripts": {
		"build": "webpack --config webpack.prod.js",
		"prod": "npm run build && http-server ./dist -a 127.0.0.1 -p 8080 --cors --log-ip",
		"dev": "webpack-dev-server --color --progress --config webpack.dev.js",
		"test": "jest"
	},
	"keywords": [
		"blog",
		"vue",
		"vue-router",
		"js",
		"weblog"
	],
	"author": "Luis",
	"license": "MIT",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.12",
		"@fortawesome/free-brands-svg-icons": "^5.6.3",
		"@fortawesome/free-regular-svg-icons": "^5.6.3",
		"@fortawesome/free-solid-svg-icons": "^5.6.3",
		"@fortawesome/vue-fontawesome": "^0.1.5",
		"axios": "^0.19.0",
		"body-parser": "^1.18.3",
		"copy-webpack-plugin": "^5.0.2",
		"dompurify": "^2.0.7",
		"express": "^4.16.4",
		"http-server": "^0.12.1",
		"lodash": "^4.17.19",
		"marked": "^0.7.0",
		"material-design-icons-iconfont": "^4.0.4",
		"vue": "^2.5.22",
		"vuetify": "^1.4.3",
		"vuex": "^3.1.0"
	},
	"devDependencies": {
		"@mdi/font": "^3.9.97",
		"@vue/test-utils": "^1.0.0-beta.31",
		"babel-core": "^6.26.3",
		"babel-jest": "^24.9.0",
		"babel-preset-env": "^1.7.0",
		"css-loader": "^2.1.1",
		"file-loader": "^3.0.1",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.9.0",
		"node-sass": "^4.13.1",
		"sass-loader": "^7.3.1",
		"style-loader": "^0.23.1",
		"vue-jest": "^3.0.5",
		"vue-loader": "^15.8.3",
		"vue-router": "^3.1.5",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.6.11",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.1",
		"webpack-merge": "^4.2.2"
	},
	"jest": {
		"bail": false,
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"transform": {
			".*\\.(vue)$": "vue-jest",
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest"
		},
		"verbose": true
	}
}
